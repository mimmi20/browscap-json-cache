{
  "name": "mimmi20/browscap-json-cache",
  "type": "application",
  "description": "Browser Capabilities Json Cache",
  "keywords": [
    "browscap"
  ],
  "homepage": "https://github.com/mimmi20/browscap-json-cache",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-json-cache/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "mimmi20/wurflcache": "^1.7.1.1"
  },
  "require-dev": {
    "browscap/browscap-php": "3.x-dev as 3.2.0",
    "ergebnis/composer-normalize": "^2.2.2",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "phpstan/phpstan": "^0.12.8",
    "squizlabs/php_codesniffer": "^3.5.3"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Browscap\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapTest\\Cache\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mimmi20/browscap-php"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/browscap-json-cache/issues",
    "source": "https://github.com/mimmi20/browscap-json-cache"
  }
}
